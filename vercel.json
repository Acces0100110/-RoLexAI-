{
  "version": 2,
  "builds": [
    {
      "src": "frontend/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/web/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/web/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://rolexai.onrender.com/api"
  }
}
